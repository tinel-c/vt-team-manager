doctype html
head
  meta(charset='utf-8')
  meta(name='viewport' content='width=device-width, initial-scale=1, shrink-to-fit=no')
  meta(name='description' content='Start your development with a Dashboard for Bootstrap 4.')
  meta(name='author' content='Creative Tim')
  title Argon Dashboard - Free Dashboard for Bootstrap 4
  // Favicon
  link(rel='icon' href='assets/img/brand/favicon.png' type='image/png')
  // Fonts
  link(rel='stylesheet' href='https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700')
  // Icons
  link(rel='stylesheet' href='assets/vendor/nucleo/css/nucleo.css' type='text/css')
  link(rel='stylesheet' href='assets/vendor/@fortawesome/fontawesome-free/css/all.min.css' type='text/css')
  // Page plugins
  // Argon CSS
  link(rel='stylesheet' href='assets/css/argon.css?v=1.2.0' type='text/css')
  include side-navigation.pug

// Main content
  block notifications
#panel.main-content
  include top-nav.pug
  include page-header.pug
  // Page content
  .container-fluid.mt--6
    .row
      .col-xl-8
        .card.bg-default
          .card-header.bg-transparent
            .row.align-items-center
              .col
                h6.text-light.text-uppercase.ls-1.mb-1 Overview
                h5.h3.text-white.mb-0 Sales value
              .col
                ul.nav.nav-pills.justify-content-end
                  li.nav-item.mr-2.mr-md-0(data-toggle='chart' data-target='#chart-sales-dark' data-update='{"data":{"datasets":[{"data":[0, 20, 10, 30, 15, 40, 20, 60, 60]}]}}' data-prefix='$' data-suffix='k')
                    a.nav-link.py-2.px-3.active(href='#' data-toggle='tab')
                      span.d-none.d-md-block Month
                      span.d-md-none M
                  li.nav-item(data-toggle='chart' data-target='#chart-sales-dark' data-update='{"data":{"datasets":[{"data":[0, 20, 5, 25, 10, 30, 15, 40, 40]}]}}' data-prefix='$' data-suffix='k')
                    a.nav-link.py-2.px-3(href='#' data-toggle='tab')
                      span.d-none.d-md-block Week
                      span.d-md-none W
          .card-body
            // Chart
            .chart
              // Chart wrapper
              canvas#chart-sales-dark.chart-canvas
      .col-xl-4
        .card
          .card-header.bg-transparent
            .row.align-items-center
              .col
                h6.text-uppercase.text-muted.ls-1.mb-1 Performance
                h5.h3.mb-0 Total orders
          .card-body
            // Chart
            .chart
              canvas#chart-bars.chart-canvas
    .row
      .col-xl-8
        .card
          .card-header.border-0
            .row.align-items-center
              .col
                h3.mb-0 Page visits
              .col.text-right
                a.btn.btn-sm.btn-primary(href='#!') See all
          .table-responsive
            // Projects table
            table.table.align-items-center.table-flush
              thead.thead-light
                tr
                  th(scope='col') Page name
                  th(scope='col') Visitors
                  th(scope='col') Unique users
                  th(scope='col') Bounce rate
              tbody
                tr
                  th(scope='row')
                    | /argon/
                  td
                    | 4,569
                  td
                    | 340
                  td
                    i.fas.fa-arrow-up.text-success.mr-3
                    |  46,53%
                tr
                  th(scope='row')
                    | /argon/index.html
                  td
                    | 3,985
                  td
                    | 319
                  td
                    i.fas.fa-arrow-down.text-warning.mr-3
                    |  46,53%
                tr
                  th(scope='row')
                    | /argon/charts.html
                  td
                    | 3,513
                  td
                    | 294
                  td
                    i.fas.fa-arrow-down.text-warning.mr-3
                    |  36,49%
                tr
                  th(scope='row')
                    | /argon/tables.html
                  td
                    | 2,050
                  td
                    | 147
                  td
                    i.fas.fa-arrow-up.text-success.mr-3
                    |  50,87%
                tr
                  th(scope='row')
                    | /argon/profile.html
                  td
                    | 1,795
                  td
                    | 190
                  td
                    i.fas.fa-arrow-down.text-danger.mr-3
                    |  46,53%
      .col-xl-4
        .card
          .card-header.border-0
            .row.align-items-center
              .col
                h3.mb-0 Social traffic
              .col.text-right
                a.btn.btn-sm.btn-primary(href='#!') See all
          .table-responsive
            // Projects table
            table.table.align-items-center.table-flush
              thead.thead-light
                tr
                  th(scope='col') Referral
                  th(scope='col') Visitors
                  th(scope='col')
              tbody
                tr
                  th(scope='row')
                    | Facebook
                  td
                    | 1,480
                  td
                    .d-flex.align-items-center
                      span.mr-2 60%
                      div
                        .progress
                          .progress-bar.bg-gradient-danger(role='progressbar' aria-valuenow='60' aria-valuemin='0' aria-valuemax='100' style='width: 60%;')
                tr
                  th(scope='row')
                    | Facebook
                  td
                    | 5,480
                  td
                    .d-flex.align-items-center
                      span.mr-2 70%
                      div
                        .progress
                          .progress-bar.bg-gradient-success(role='progressbar' aria-valuenow='70' aria-valuemin='0' aria-valuemax='100' style='width: 70%;')
                tr
                  th(scope='row')
                    | Google
                  td
                    | 4,807
                  td
                    .d-flex.align-items-center
                      span.mr-2 80%
                      div
                        .progress
                          .progress-bar.bg-gradient-primary(role='progressbar' aria-valuenow='80' aria-valuemin='0' aria-valuemax='100' style='width: 80%;')
                tr
                  th(scope='row')
                    | Instagram
                  td
                    | 3,678
                  td
                    .d-flex.align-items-center
                      span.mr-2 75%
                      div
                        .progress
                          .progress-bar.bg-gradient-info(role='progressbar' aria-valuenow='75' aria-valuemin='0' aria-valuemax='100' style='width: 75%;')
                tr
                  th(scope='row')
                    | twitter
                  td
                    | 2,645
                  td
                    .d-flex.align-items-center
                      span.mr-2 30%
                      div
                        .progress
                          .progress-bar.bg-gradient-warning(role='progressbar' aria-valuenow='30' aria-valuemin='0' aria-valuemax='100' style='width: 30%;')
    // Footer
    include footer.pug
// Argon Scripts
// Core
script(src='assets/vendor/jquery/dist/jquery.min.js')
script(src='assets/vendor/bootstrap/dist/js/bootstrap.bundle.min.js')
script(src='assets/vendor/js-cookie/js.cookie.js')
script(src='assets/vendor/jquery.scrollbar/jquery.scrollbar.min.js')
script(src='assets/vendor/jquery-scroll-lock/dist/jquery-scrollLock.min.js')
// Optional JS
script(src='assets/vendor/chart.js/dist/Chart.min.js')
script(src='assets/vendor/chart.js/dist/Chart.extension.js')
// Argon JS
script(src='assets/js/argon.js?v=1.2.0')
