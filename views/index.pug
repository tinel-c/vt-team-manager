extends layout

block content
    // Page content
    .container-fluid.mt--6
        .row
          .col-xl-8
            .card.bg-default
              .card-header.bg-transparent
                .row.align-items-center
                  .col
                    h6.text-light.text-uppercase.ls-1.mb-1 Overview
                    h5.h3.text-white.mb-0 Sales value
                  .col
                    ul.nav.nav-pills.justify-content-end
                      li.nav-item.mr-2.mr-md-0(data-toggle='chart' data-target='#chart-sales-dark' data-update='{"data":{"datasets":[{"data":[0, 10, 50, 20, 10, 30, 15, 40, 20, 60, 60]}]}}' data-prefix='$' data-suffix='k')
                        a.nav-link.py-2.px-3.active(href='#' data-toggle='tab')
                          span.d-none.d-md-block Month
                          span.d-md-none M
                      li.nav-item(data-toggle='chart' data-target='#chart-sales-dark' data-update='{"data":{"datasets":[{"data":[0, 20, 5, 25, 10, 30, 15, 40, 40]}]}}' data-prefix='$' data-suffix='k')
                        a.nav-link.py-2.px-3(href='#' data-toggle='tab')
                          span.d-none.d-md-block Week
                          span.d-md-none W
              .card-body
                // Chart
                .chart
                  // Chart wrapper
                  canvas#chart-sales-dark.chart-canvas
          .col-xl-4
            .card
              .card-header.bg-transparent
                .row.align-items-center
                  .col
                    h6.text-uppercase.text-muted.ls-1.mb-1 Performance
                    h5.h3.mb-0 Total orders
              .card-body
                // Chart
                .chart
                  canvas#chart-bars.chart-canvas
        .row
          .col-xl-8
            .card
              .card-header.border-0
                .row.align-items-center
                  .col
                    h3.mb-0 Page visits
                  .col.text-right
                    a.btn.btn-sm.btn-primary(href='#!') See all
              .table-responsive
                // Projects table
                table.table.align-items-center.table-flush
                  thead.thead-light
                    tr
                      th(scope='col') Page name
                      th(scope='col') Visitors
                      th(scope='col') Unique users
                      th(scope='col') Bounce rate
                  tbody
                    tr
                      th(scope='row')
                        | /argon/
                      td
                        | 4,569
                      td
                        | 340
                      td
                        i.fas.fa-arrow-up.text-success.mr-3
                        |  46,53%
