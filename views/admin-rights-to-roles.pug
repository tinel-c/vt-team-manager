  // Page content
  form(role='form' action="/admin-rights-to-roles" method="post")
    div.wrapper
      div.box1
        table(id='example-table')
          tbody.verticalScroll
            p.card-titles Users
            div
              -if (role[0].medical_check_three_dots_popup==0)
                input#scales(type='checkbox' value="1" name='user_medical_check_three_dots_popup')
              -else
                input#scales(type='checkbox' value="1" name='user_medical_check_three_dots_popup' checked='')
              label(for='user_medical_check_three_dots_popup') medical check three dots popup
      div.boxTL
        table(id='example-table')
          tbody.verticalScroll
            p.card-titles Team Leaders
            div
              -if (role[2].medical_check_three_dots_popup==0)
                input#scales(type='checkbox' value="1" name='teamLeader_medical_check_three_dots_popup')
              -else
                input#scales(type='checkbox' value="1" name='teamLeader_medical_check_three_dots_popup' checked='')
              label(for='teamLeader_medical_check_three_dots_popup') medical check three dots popup  
      div.box2
        table(id='example-table')
          tbody.verticalScroll
            p.card-titles Admins
            div
              -if (role[1].medical_check_three_dots_popup==0)
                input#scales(type='checkbox' value="1" name='admin_medical_check_three_dots_popup')
              -else
                input#scales(type='checkbox' value="1" name='admin_medical_check_three_dots_popup' checked='')
              label(for='admin_medical_check_three_dots_popup') medical check three dots popup
      div.box3
        table(id='example-table')
          tbody.verticalScroll
            p.card-titles Medics
            div
              -if (role[3].medical_check_three_dots_popup==0)
                input#scales(type='checkbox' value="1" name='medic_medical_check_three_dots_popup')
              -else
                input#scales(type='checkbox' value="1" name='medic_medical_check_three_dots_popup' checked='')
              label(for='medic_medical_check_three_dots_popup') medical check three dots popup
      div.boxESH
        table(id='example-table')
          tbody.verticalScroll
            p.card-titles ESH Managers
            div
              -if (role[4].medical_check_three_dots_popup==0)
                input#scales(type='checkbox' value="1" name='eshManager_medical_check_three_dots_popup')
              -else
                input#scales(type='checkbox' value="1" name='eshManager_medical_check_three_dots_popup' checked='')
              label(for='eshManager_medical_check_three_dots_popup') medical check three dots popup
